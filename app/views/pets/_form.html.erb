<%= simple_form_for pet do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :found_on %>
  <%= f.input :species, collection: [1,2,3]%>

  <%#= f.label :name %>
  <%#= f.text_field :name %>
  <%#= f.label :address %>
  <%#= f.text_field :address %>
  <%#= f.label :species %>
  <%#= f.select :species, Pet::SPECIES %>
  <%#= f.label :found_on %>
  <%#= f.date_field :found_on %>
  <%= f.submit %>
<% end %>
